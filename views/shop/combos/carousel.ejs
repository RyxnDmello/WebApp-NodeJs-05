<% for(let i = 0; i < 5; i++) { %>
<form class="combos-slide" target="/shop/item/add" method="post">
  <div class="combos-slide-images">
    <img
      class="combos-slide-image"
      src="/shop/playstation/consoles/skin-1.png"
    />
    <img
      class="combos-slide-image"
      src="/shop/playstation/consoles/skin-2.png"
    />
  </div>

  <div class="combos-slide-information">
    <div class="combos-slide-names">
      <h4 class="combos-slide-name">BROKEN LAVA</h4>
      <h4 class="combos-slide-name">BLUE FUSION</h4>
    </div>

    <div class="combos-slide-details">
      <div class="combos-slide-sponsor">Sony X CyberVerse</div>
      <div class="combos-slide-price">$800</div>
    </div>

    <div class="combos-slide-buttons">
      <button class="combos-slide-button" type="submit" name="add" value="CART">
        <img
          class="combos-slide-button-image"
          src="/buttons/shop/cart-add.png"
        />

        <p class="combos-slide-button-text">Add To Cart</p>
      </button>

      <button class="combos-slide-button" type="submit" name="add" value="WISH">
        <img class="combos-slide-button-image" src="/buttons/shop/wish.png" />
        <p class="combos-slide-button-text">Wishlist</p>
      </button>
    </div>
  </div>
</form>
<% } %>
