<div class="footer-header">
  <div class="footer-socials-container">
    <%- include("./footer/socials.ejs", { socials: footer.socials }) %>
  </div>
</div>

<div class="footer-body">
  <div class="footer-logo-container"><%- include("./footer/logo.ejs") %></div>

  <% footer.consoles.forEach((console) => { %>
  <div class="footer-console-container">
    <%- include("./footer/console.ejs", { console: console }) %>
  </div>
  <% }) %>

  <div class="footer-register-container">
    <%- include("./footer/register.ejs", { socials: footer.socials }) %>
  </div>
</div>

<img class="footer-background" src="/home/footer/background.jpg" />
